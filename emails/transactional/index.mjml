        max-width: 517px;
      }

      .border-wrapper {
        padding: 21px 3.6% 21px;
        border: 1px solid #e1e3df;
        border-top-left-radius: 6px;
        border-top-right-radius: 6px;
        border-bottom: 0;
      }

      .full-border-bottom {
        border-bottom: auto;
        border-radius: 6px;
      }

      .bottom-border-section {
        border: 1px solid #e1e3df;
        border-top: 0;
        border-bottom-left-radius: 6px;
        border-bottom-right-radius: 6px;
      }

      .inner-bottom-border-section {
        padding: 10px;
        background: #f4f4f3;
        border-bottom-left-radius: 6px;
        border-bottom-right-radius: 6px;
      }

      .carbon-image {
        vertical-align: middle;
      }

      .product-title {
        display: block;
        font-size: 16px;
        line-height: 16px;
        font-family: Arial, sans-serif;
        white-space: nowrap;
        text-overflow: ellipsis;
        color: #222222;
        overflow: hidden;
        padding: 0;
        margin: 0;
        padding-bottom: 4px;
        text-align: left;
      }

      .product-details {
        font-weight: 300;
        font-size: 14px;
        line-height: 24px;
        color: #222222;
        padding: 0;
        margin: 0;
        text-align: left;
      }

      .bold,
