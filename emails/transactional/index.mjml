    <mj-style>
      * {
        box-sizing: border-box;
        font-family: Arial, Helvetica, sans-serif;
      }

      .dark-text {
        max-width: 517px;
      }

      @media screen and (max-width: 550px) {

        .large-device-menu,
        .large-device-product-table {
          display: none;
        }

        .small-device-menu,
        .small-device-product-table {
          display: block !important;
        }

        .top-menu-links {
          font-size: 12px !important;
        }
      }

      @media screen and (max-width: 500px) {
        .large-device-info-table {
          display: none;
        }

        .small-device-info-table {
          display: block !important;
        }
      }
    </mj-style>
