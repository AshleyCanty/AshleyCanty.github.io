        padding: 10px 0 !important;
      }

      .normal-text {
        padding: 0;
        text-align: left;
        font-size: 16px;
        font-style: normal;
        line-height: 24px;
        color: #222222;
      }

      .one-third-snippet {
        max-width: 172px;
      }

      .icons-size {
        max-width: 190px;
        padding: 20px 0 10px;
      }

      .bg-image-data {
